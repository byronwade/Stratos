{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}!",
    "src/app/**/layout.tsx",
    "src/app/**/page.tsx"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "**/*.d.ts",
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "**/test/**",
    "**/tests/**",
    "**/__tests__/**",
    "node_modules/**",
    "src/components/ui/**",
    "src/components/chat/**",
    "src/lib/ai/examples.tsx",
    "src/lib/store/examples.tsx",
    "src/hooks/use-mobile.ts",
    "src/components/layout/index.ts"
  ],
  "ignoreDependencies": [
    "@types/*",
    "ultracite",
    "knip",
    "ts-prune",
    "depcheck",
    "madge",
    "@radix-ui/*",
    "class-variance-authority",
    "clsx",
    "tailwind-merge",
    "lucide-react",
    "cmdk",
    "vaul",
    "sonner",
    "input-otp",
    "react-day-picker",
    "react-hook-form",
    "@hookform/resolvers",
    "zod",
    "zod-to-json-schema",
    "framer-motion",
    "embla-carousel-react",
    "react-resizable-panels",
    "recharts",
    "date-fns",
    "nanoid",
    "classnames",
    "fast-deep-equal",
    "usehooks-ts",
    "use-stick-to-bottom",
    "swr",
    "shiki",
    "react-syntax-highlighter",
    "streamdown",
    "resumable-stream",
    "papaparse",
    "diff-match-patch",
    "orderedmap",
    "codemirror",
    "@codemirror/*",
    "prosemirror-*",
    "react-data-grid",
    "next-themes",
    "@vercel/blob",
    "@supabase/*",
    "bcrypt-ts",
    "ai",
    "@ai-sdk/*"
  ],
  "next": {
    "entry": [
      "src/app/**/*.tsx",
      "src/app/**/*.ts",
      "!src/app/**/*.test.{ts,tsx}",
      "!src/app/**/*.spec.{ts,tsx}"
    ]
  }
}
